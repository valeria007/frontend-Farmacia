                                                    <label class="control-label">Establecimiento</label>
                                                    <select class="form-control">
                                                        <option>HOSP. D. BRACAMONTE</option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">Distribucion por Pacientes</label>
                                                    <select class="form-control">
                                                        <option>SI</option>
                                                        <option>NO</option>
                                                    </select>
                                                </div>
                                                <div class="form-group label-floating">
                                                    <label class="control-label">Lapso de Reabastecimiento</label>
                                                    <input class="form-control" type="text">
                                                </div>
                                                <div class="form-group label-floating">
                                                    <label class="control-label">Nivel Minimo</label>
                                                    <input class="form-control" type="text">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group label-floating">
                                                    <label class="control-label">Nivel de Reserva</label>
                                                    <input class="form-control" type="text">
                                                </div>
                                                <div class="form-group label-floating">
                                                    <label class="control-label">Nivel Maximo</label>
                                                    <input class="form-control" type="text">
                                                </div>
                                                <div class="form-group label-floating">
                                                    <label class="control-label">Periodo de Reabastecimiento</label>
                                                    <input class="form-control" type="text">
                                                </div>
                                                <div class="form-group label-floating">
                                                    <label class="control-label">MÃ©todo de Costeo</label>
                                                    <input class="form-control" type="text">
                                                </div>                             
                                            </div>
                                        </div>
                                    </form>
                                    <p class="text-center">
                                        <button type="reset" class="btn btn-info" style="margin-right: 20px;"><i class="zmdi zmdi-roller"></i> &nbsp;&nbsp; Limpiar</button>
                                       <button type="submit" class="btn btn-danger"><i class="zmdi zmdi-floppy"></i> &nbsp;&nbsp; Guardar</button>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
				    <!-- Nuevo Medicamento -->
                    <div class="tab-pane fade" id="list">
                        <div class="table-responsive">
                            <table class="table table-hover text-center">
                                <thead>
                                    <tr>
                                        <th class="text-center">#</th>
                                        <th class="text-center">Almacen</th>
                                        <th class="text-center">Establecimiento</th>
                                        <th class="text-center">Lapso de Reabastecimiento</th>
                                        <th class="text-center">Nivel de Reserva</th>
                                        <th class="text-center">Periodo Reabastecimiento</th>
                                        <th class="text-center">Nivel de Minimo</th>
                                        <th class="text-center">Nivel de Maximo</th>
